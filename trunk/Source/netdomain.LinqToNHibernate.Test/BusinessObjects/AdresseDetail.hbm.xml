<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="netdomain.LinqToNHibernate.Test.BusinessObjects"
                   assembly="netdomain.LinqToNHibernate.Test">

    <class name="AdresseDetail" table="`AdresseDetail`">
        <id name="Id" column="Id" type="Int32" unsaved-value="0">
            <generator class="identity"/>
        </id>

        <property name="AdresseId" column="`AdresseId`" />

        <many-to-one name="Adresse" column="AdresseId" foreign-key="FK_AdresseDetail_Adresse"/>
    </class>    
</hibernate-mapping>